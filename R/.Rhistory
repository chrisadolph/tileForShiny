traces <- list(trace1=trace1,trace2=trace2,trace3=trace3,trace=trace4,vertmark=vertmark)# Create tile plot and save to pdf tc <- tile(#trace1,trace2,trace3, trace4,           #vertmark,traces,           output = list(width=7,outfile="ropeladderEx1"),           xaxis = list(rug = list(data=UScrime$y - mean(UScrime$y))),           xaxistitle = list(labels="Expected crime rate (per 100,000)"),           topaxis= list(at=mean(UScrime$y)*c(0.5, 1, 1.5, 2) - mean(UScrime$y),                         labels=c("0.5x","1x","1.5x","2x"),                         add=c(TRUE,FALSE)                         ),           topaxistitle = list(labels="Expected crime rate (x average)"),           gridlines=list(type="top"),           verbose=FALSE           )# Draw a second copy of the plot to the screentileDraw(tc)
